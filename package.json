{
  "neutrino": {
    "use": [
      "neutrino-preset-karma",
      "neutrino-preset-airbnb-base",
      "neutrino-preset-react",
      "neutrino-preset-postcss"
    ]
  },
  "scripts": {
    "start": "neutrino start",
    "build": "neutrino build",
    "server": "npm-watch",
    "ssr": "neutrino build; babel-node server/index.js"
  },
  "watch": {
    "ssr": "{src,test}/*.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "postcss": {
    "parser": "sugarss",
    "map": false,
    "from": "src/*.css",
    "to": "build/dest.css"
  },
  "dependencies": {
    "babel-plugin-transform-assets": "^0.2.0",
    "htmltidy": "^0.0.6",
    "lodash": "^4.17.4",
    "npm-watch": "^0.1.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "express": "^4.15.2",
    "express-react-views": "^0.10.2",
    "jsdom": "^9.12.0",
    "neutrino": "^5.0.0",
    "neutrino-preset-airbnb-base": "^4.3.1",
    "neutrino-preset-karma": "^4.2.0",
    "neutrino-preset-postcss": "^1.0.0",
    "neutrino-preset-react": "^4.2.3",
    "postcss": "^5.2.16",
    "postcss-load-config": "^1.2.0",
    "suitcss": "^2.0.0",
    "suitcss-preprocessor": "^3.0.1"
  }
}
